{
	"rules": {
		"no-unused-expression": true,
		"no-duplicate-variable": true,
		"curly": true,
		"class-name": true,
		"semicolon": ["always"],
		"no-unused-variable": [true, {"ignore-pattern": "^_"}],
		"triple-equals": true,
		"prefer-const": true,
		"trailing-comma": [true, {"multiline": "always", "singleline": "never"}]
	}
}
